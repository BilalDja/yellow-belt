{
  "name": "yellow-belt",
  "version": "0.0.2",
  "description": "Express typescript boilerplate project that gives you a boost and a nice workflow",
  "main": "index.js",
  "scripts": {
    "gen-env": "gen-env-types .env -o src/types/env.d.ts -e .",
    "test": "nodemon -r dotenv-safe/config tests.ts",
    "dev": "nodemon -r dotenv-safe/config src/index.ts",
    "clean": "rd /s /q dist >nul 2>&1|echo.>nul",
    "build": "yarn clean && tsc",
    "start": "node -r dotenv-safe/config dist/index.js"
  },
  "keywords": [
    "Boilerplate",
    "Express",
    "Typescript",
    "mongodb",
    "mongoose",
    "mongodb-memory",
    "logging",
    "morgan",
    "passport",
    "Mocha",
    "chai"
  ],
  "author": "BILAL Djaghout <b.djaghout@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/argon2": "^0.15.0",
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.8",
    "@types/joi": "^14.3.4",
    "@types/mocha": "^8.0.3",
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.4",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "chai": "^4.2.0",
    "gen-env-types": "^1.0.3",
    "mocha": "^8.1.3",
    "mongodb-memory-server": "^6.6.7",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "argon2": "^0.27.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.3",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "remove": "^0.1.5",
    "rotating-file-stream": "^2.1.3"
  },
  "repository": {
    "url": "https://github.com/BilalDja/yellow-belt",
    "type": "git"
  },
  "homepage": "https://github.com/BilalDja/yellow-belt",
  "bugs": {
    "url": "https://github.com/BilalDja/yellow-belt/issues",
    "email": "b.djaghout@gmail.com"
  }
}
